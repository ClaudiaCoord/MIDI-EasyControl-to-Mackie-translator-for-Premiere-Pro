print("Example: Light RGB led effects...")
var group = CreateRGB(129,10,11,12);
group.SetColor(120);

while (!IsCanceled()) {

  for (var light = 0; light <= 100; light += 10) {

    if (IsCanceled()) { break; }
    group.SetLight(light);
    group.ApplyValues();

    if (IsCanceled()) { break; }
    sleep(2500);
    
  }
}

